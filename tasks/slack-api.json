{
  "containerDefinitions": [
    {
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "name": "slack-api",
      "image": "databraiddb/slack-api:latest",
      "cpu": 10,
      "command": ["npm", "start"]
    }
  ],
  "family": "slack-api"
}